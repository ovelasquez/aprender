{# empty Twig template #}
{% block form_errors %}
    {% spaceless %}
        {% if errors|length > 0 %}
        <ul class="alert alert-danger list-unstyled">
            {% for error in errors %}
                <li>{{ error.message }}</li>
            {% endfor %}
        </ul>
        {% endif %}
    {% endspaceless %}
{% endblock form_errors %}